<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>AI Model Benchmark Results</title>
    <script src="https://cdn.tailwindcss.com"></script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
</head>
<body class="bg-gray-50">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-12">
        <h1 class="text-3xl font-bold text-gray-900 mb-8">AI Model Benchmark Results</h1>
        
        <!-- Summary Cards -->
        <div class="grid grid-cols-1 md:grid-cols-3 gap-6 mb-12">
            <div class="bg-white rounded-lg shadow p-6">
                <h3 class="text-lg font-medium text-gray-900 mb-2">Top Performing Model</h3>
                <p class="text-2xl font-bold text-indigo-600">
                    {% set top_model = (proprietary_models + open_source_models)|sort(attribute='score')|reverse|first %}
                    {{ top_model.name }} ({{ top_model.score }}%)
                </p>
            </div>
            <div class="bg-white rounded-lg shadow p-6">
                <h3 class="text-lg font-medium text-gray-900 mb-2">Average Score</h3>
                <p class="text-2xl font-bold text-indigo-600">
                    {% set avg_score = ((proprietary_models + open_source_models)|map(attribute='score')|sum / (proprietary_models|length + open_source_models|length))|round %}
                    {{ avg_score }}%
                </p>
            </div>
            <div class="bg-white rounded-lg shadow p-6">
                <h3 class="text-lg font-medium text-gray-900 mb-2">Models Tested</h3>
                <p class="text-2xl font-bold text-indigo-600">
                    {{ proprietary_models|length + open_source_models|length }}
                </p>
            </div>
            <div class="bg-white rounded-lg shadow p-6">
                <h3 class="text-lg font-medium text-gray-900 mb-2">Fastest Response</h3>
                <p class="text-2xl font-bold text-indigo-600">
                    {% set fastest_model = (proprietary_models + open_source_models)|sort(attribute='first_token_latency')|first %}
                    {{ fastest_model.name }} ({{ fastest_model.first_token_latency }})
                </p>
            </div>
        </div>

        <!-- Performance Chart -->
        <div class="bg-white rounded-lg shadow p-6 mb-12">
            <h2 class="text-xl font-bold text-gray-900 mb-4">Model Performance Comparison</h2>
            <canvas id="performanceChart" height="100"></canvas>
        </div>

        <!-- Detailed Results Tables -->
        <div class="grid grid-cols-1 lg:grid-cols-2 gap-8">
            <!-- Proprietary Models -->
            <div class="bg-white rounded-lg shadow">
                <div class="px-6 py-4 border-b border-gray-200">
                    <h3 class="text-lg font-medium text-gray-900">Proprietary Models</h3>
                </div>
                <div class="overflow-x-auto">
                    <table class="min-w-full divide-y divide-gray-200">
                        <thead class="bg-gray-50">
                            <tr>
                                <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Model</th>
                                <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Score</th>
                                <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">First Token Latency</th>
                                <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Function Call Support</th>
                            </tr>
                        </thead>
                        <tbody class="bg-white divide-y divide-gray-200">
                            {% for model in proprietary_models %}
                            <tr>
                                <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">{{ model.name }}</td>
                                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">{{ model.score }}%</td>
                                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">{{ model.first_token_latency }}</td>
                                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">{{ model.function_call_support|safe }}</td>
                            </tr>
                            {% endfor %}
                        </tbody>
                    </table>
                </div>
            </div>

            <!-- Open Source Models -->
            <div class="bg-white rounded-lg shadow">
                <div class="px-6 py-4 border-b border-gray-200">
                    <h3 class="text-lg font-medium text-gray-900">Open Source Models</h3>
                </div>
                <div class="overflow-x-auto">
                    <table class="min-w-full divide-y divide-gray-200">
                        <thead class="bg-gray-50">
                            <tr>
                                <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Model</th>
                                <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Score</th>
                                <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">First Token Latency</th>
                                <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Function Call Support</th>
                            </tr>
                        </thead>
                        <tbody class="bg-white divide-y divide-gray-200">
                            {% for model in open_source_models %}
                            <tr>
                                <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">{{ model.name }}</td>
                                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">{{ model.score }}%</td>
                                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">{{ model.first_token_latency }}</td>
                                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">{{ model.function_call_support|safe }}</td>
                            </tr>
                            {% endfor %}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

    <script>
        const ctx = document.getElementById('performanceChart').getContext('2d');
        new Chart(ctx, {
            type: 'bar',
            data: {
                labels: [
                    {% for model in proprietary_models + open_source_models %}
                        '{{ model.name }}',
                    {% endfor %}
                ],
                datasets: [{
                    label: 'Performance Score (%)',
                    data: [
                        {% for model in proprietary_models + open_source_models %}
                            {{ model.score }},
                        {% endfor %}
                    ],
                    backgroundColor: [
                        {% for model in proprietary_models %}
                            'rgba(79, 70, 229, 0.8)',
                        {% endfor %}
                        {% for model in open_source_models %}
                            'rgba(16, 185, 129, 0.8)',
                        {% endfor %}
                    ]
                }]
            },
            options: {
                responsive: true,
                scales: {
                    y: {
                        beginAtZero: true,
                        max: 100
                    }
                }
            }
        });
    </script>
</body>
</html> 